ALGO_NAME = edf
ARCH_NAME = aarch64
MACHINE_NAME = raspi3
CC = clang
OUTPUT_NAME = uart
CFLAGS +=  \
-DCONFIG_ARCH_AARCH64 \
-DCONFIG_ARCH_AARCH64_RASPI3 \
-DENABLE_FPU \
-DCONFIG_COMPILER_CLANG \
-DCONFIG_PRINT2UART \
-DCONFIG_ALGO_EDF \
-DSCHED_DP \
-DCONFIG_TQ_DLIST \
-DCONFIG_TIE_BREAK_FIFO \
-DCONFIG_OPTION_DEBUG \
-DCONFIG_OPTION_FS_FAT

SRCS +=  \
$(TOP_DIR)/kernel/queue/dlist.c \
$(TOP_DIR)/fs/fat/fat.c

ASRCS += 
