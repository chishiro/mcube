ALGO_NAME = fp
ARCH_NAME = aarch64
MACHINE_NAME = raspi3
CC = gcc
OUTPUT_NAME = uart
CFLAGS +=  \
-DCONFIG_ARCH_AARCH64 \
-DCONFIG_ARCH_AARCH64_RASPI3 \
-DENABLE_FPU \
-DCONFIG_COMPILER_GCC \
-DCONFIG_PRINT2UART \
-DCONFIG_ALGO_FP \
-DSCHED_FP \
-DCONFIG_TQ_O1 \
-DCONFIG_TIE_BREAK_FIFO \
-DCONFIG_OPTION_DEBUG

SRCS +=  \
$(TOP_DIR)/kernel/queue/o1.c

ASRCS += 
